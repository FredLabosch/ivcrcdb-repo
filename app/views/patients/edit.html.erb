<% provide(:title, "Edit patient") %>
<h1>Update patient information</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for @patient do |f| %>
        <%= render 'fields', :f => f %>

        <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>